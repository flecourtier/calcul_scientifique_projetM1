set title "Résolution de l'équation de Burgers (rusanov): t=0.5"
set key left top
plot 'resu_rusanov_t05_N50.dat' using 1:3 w l title 'u_{exact}',\
'resu_rusanov_t05_N50.dat' using 1:2 w l title 'N=50' ,\
'resu_rusanov_t05_N100.dat' using 1:2 w l title 'N=100',\
'resu_rusanov_t05_N1000.dat' using 1:2 w l title 'N=1000',\
'resu_rusanov_t05_N10000.dat' using 1:2 w l title 'N=10000'
pause -1

